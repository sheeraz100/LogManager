{
    "name": "sheeraz/logmanager",
    "description": "Preview Log files, Download log files and Send mail to the registered users",
    "keywords": [
        "backpack",
        "log manager",
        "logs",
        "laravel",
        "log viewer"
    ],
    "homepage": "https://github.com/sheeraz100/LogManager",
    "license": "proprietary",
    "authors": [
        {
            "name": "Sheeraz",
            "email": "kleensoft15@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "backpack/crud": "^4.1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.0||^7.0",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Sheeraz\\LogManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sheeraz\\LogManager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Sheeraz\\LogManager\\LogManagerServiceProvider"
            ]
        }
    }
}
